<div class="row">
  <div class="col-lg-2 left">
    <div class="panel-group">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h4 class="panel-title">
            <a data-toggle="collapse" href="#collapse1">Maps  <span class="glyphicon glyphicon-chevron-down"></span></a>
          </h4>
        </div>
        <div id="collapse1" class="panel-collapse collapse">
          <ul class="list-group">
          <% for (let i in all_maps) { %>
            <li class="list-group-item"><%= all_maps[i] %></li>
          <% } %>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <% if (user.name) {%>
  <div id='overlay' class="col-lg-6">
    <div id='overlay_header'>
      <div class="close">
        <span id='overlay_close' class="glyphicon glyphicon-remove"></span>
      </div>
      This is the header
    </div>
    <div id='addList_view'>
      <% include _addList %>
    </div>
    <div id='favourites_view'>
      <% include _favourites %>
    </div>
  </div>
    <div class="col-lg-2 right">
      <div class="panel-group">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h4 class="panel-title">
              <a data-toggle="collapse" href="#collapse2">Hello <%=user.name%>  <span class="glyphicon glyphicon-chevron-down"></span></a>
            </h4>
          </div>
          <div id="collapse2" class="panel-collapse collapse">
            <ul class="list-group">
              <li class="list-group-item"><a id='addList' href=''>Add a List</a></li>
              <li class="list-group-item"><a id='favourites' href=''>Favourites</a></li>
              <li class="list-group-item"><a id='logout' href=''>Log out</a></li>
            </ul>
            <!-- <div class="panel-footer">Footer</div> -->
          </div>
        </div>
      </div>
    </div>
  <%}%>
</div>
